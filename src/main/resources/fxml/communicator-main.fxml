<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="120.0" prefWidth="280.0" AnchorPane.bottomAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="30.0" prefWidth="200.0" spacing="5.0">
               <children>
                  <Label text="Your IP:" />
                  <Label fx:id="yourIPLabel" text="Label" />
                  <Label text="Your Port:" />
                  <Label fx:id="yourPortLabel" text="Label" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="30.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Type IP:   " />
                  <TextField fx:id="IPTextField" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="30.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Type Port:" />
                  <TextField fx:id="portTextField" prefWidth="200.0" />
               </children>
            </HBox>
            <JFXButton fx:id="newMessageButton" text="New Message" />
         </children>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
