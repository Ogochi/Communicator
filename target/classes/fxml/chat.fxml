<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="420.0" spacing="5.0">
         <children>
            <TextArea fx:id="chatBoxTextArea" editable="false" prefHeight="320.0" prefWidth="400.0" wrapText="true" />
            <Label text="Your Message:" />
            <TextField fx:id="newMessageTextField" />
         </children>
      </VBox>
   </children>
</AnchorPane>
